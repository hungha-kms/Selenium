<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNGListenerSuite">
	<listeners>
		<listener class-name="com.howtodoinjava.listeners.Listeners" />
	</listeners>
	<test name="LoginTest">
		<classes>
			<class name="com.howtodoinjava.listeners.LoginTest">
				<methods> 
					<include name="loginByEmail"/>
					<include name="loginByFacebook"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>